{
  "name": "ahoney",
  "version": "1.0.7",
  "description": "",
  "repository": "git@github.com:JasonChen-Spotec/ahoney.git",
  "main": "lib/ahoney.js",
  "style": "lib/index.css",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "dev": "ahoney-cli dev",
    "buildSvg": "svgr --template buildIconTemplate.js -d src/icons/ src/svgs",
    "test": "ahoney-cli test",
    "lint": "ahoney-cli lint",
    "build": "ahoney-cli build",
    "release": "release-it",
    "test:coverage": "open test/coverage/index.html",
    "build-site": "ahoney-cli build-site && gh-pages -d site"
  },
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{vue,css,less,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "ahoney-cli": "^1.0.7",
    "babel-plugin-import": "^1.13.0",
    "vue": "^2.6.11",
    "vue-svg-loader": "0.17.0-beta.2",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@vant"
    ]
  },
  "stylelint": {
    "extends": [
      "@vant/stylelint-config"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "dependencies": {
    "insert-css": "^2.0.0"
  },
  "release-it": {
    "git": {
      "tag": true,
      "commitMessage": "chore: release ${version}",
      "addUntrackedFiles": true,
      "requireCleanWorkingDir": false
    }
  }
}
